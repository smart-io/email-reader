{
  "name": "smart/emailreader",
  "type": "library",
  "description": "Email reader and dispatcher powered by Gearman",
  "keywords": [
    "Email",
    "Reader",
    "Dispatcher",
    "Gearman"
  ],
  "homepage": "https://github.com/smart-io/email-reader",
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Gilbert",
      "email": "kevin@kegisiteweb.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/console": "~2",
    "sinergi/token": "<1",
    "psr/log": "~1",
    "tedivm/fetch": "0.6.*",
    "willdurand/email-reply-parser": "~2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "suggest": {
    "ext-gearman": "This library needs Gearman"
  },
  "autoload": {
    "psr-4": {
      "Smart\\EmailReader\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\EmailReader\\Tests\\": ["tests/EmailReader/Tests"]
    }
  }
}
